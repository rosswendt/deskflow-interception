# Injected by CPack at package time

# Honor an explicit arch if already set; otherwise fall back to the configured build arch.
if(NOT DEFINED CPACK_WIX_ARCHITECTURE OR CPACK_WIX_ARCHITECTURE STREQUAL "")
  set(CPACK_WIX_ARCHITECTURE "@BUILD_ARCHITECTURE@")
endif()

# Ensure OS_STRING matches the final WiX arch for file naming
if(NOT DEFINED OS_STRING OR OS_STRING STREQUAL "")
  set(OS_STRING "win-@BUILD_ARCHITECTURE@")
endif()